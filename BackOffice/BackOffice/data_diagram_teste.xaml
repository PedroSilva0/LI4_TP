<Window x:Class="BackOffice.data_diagram_teste"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BackOffice"
        mc:Ignorable="d"
        Title="data_diagram_teste" Height="341.447" Width="621.711">
    <Grid>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="175" Margin="116,25,0,0" VerticalAlignment="Top" Width="399" IsManipulationEnabled="True" SelectionMode="Multiple" PreviewMouseLeftButtonUp="listView_PreviewMouseLeftButtonUp">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Nome" DisplayMemberBinding="{Binding nome}" Width="50" />
                    <GridViewColumn Header="id" DisplayMemberBinding="{Binding id_est}" Width="20" />
                    <GridViewColumn Header="Morada" DisplayMemberBinding="{Binding morada}" Width="0" />
                    <GridViewColumn Header="latitude" DisplayMemberBinding="{Binding latitude}" Width="0"/>
                    <GridViewColumn Header="longitude" DisplayMemberBinding="{Binding longitude}" Width="0"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
